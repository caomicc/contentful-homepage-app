  "dependencies": {
    "@contentful/app-sdk": "4.17.1",
    "@contentful/f36-components": "4.45.0",
    "@contentful/f36-tokens": "4.0.2",
    "@contentful/react-apps-toolkit": "1.2.4",
    "contentful-management": "10.10.0",
